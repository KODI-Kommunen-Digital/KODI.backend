{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "standard",
        "prettier"
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "no-var": "error",
        "camelcase": [
            "error",
            {
                "properties": "always"
            }
        ],
        "no-useless-escape": 0,
        "no-unneeded-ternary": 0,
        "indent":["error", 4],
        "no-tabs": 0,
        "import/no-unresolved": [2, { "caseSensitive": true }]
    }
}